{
  "openapi": "3.1.0",
  "info": {
    "title": "Schema Object Example Primitive Sample",
    "version": "1.0.0"
  },
  "paths": {
    "/user": {
      "get": {
        "operationId": "getUser",
        "responses": {
          "200": {
            "description": "Returns a user",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "email": {
                      "type": "string",
                      "format": "email"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
