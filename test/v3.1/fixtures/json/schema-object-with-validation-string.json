{
  "openapi": "3.1.0",
  "info": {
    "title": "Schema Object Example Simple Model",
    "version": "1.0.0"
  },
  "paths": {
    "/user": {
      "get": {
        "operationId": "getUser",
        "responses": {
          "200": {
            "description": "Returns a user",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": [
                    "name"
                  ],
                  "properties": {
                    "maxLength": {
                      "type": "string",
                      "maxLength": 100
                    },
                    "maximum": {
                      "type": "string",
                      "minLength": 1
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
